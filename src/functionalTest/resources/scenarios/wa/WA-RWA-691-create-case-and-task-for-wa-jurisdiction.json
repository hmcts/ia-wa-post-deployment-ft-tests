{
  "description": "create case and task for the WA jurisdiction",
  "enabled": true,
  "jurisdiction": "WA",
  "caseType": "WaCaseType",
  "options": {
    "taskRetrievalApi": "camunda-api"
  },
  "required": {
    "credentials": "IALegalRepresentative",
    "ccd": {
      "eventId": "CREATE",
      "state": "",
      "caseData": {
        "template": "minimal-appeal-started.json"
      }
    }
  },
  "test": {
    "request": {
      "credentials": "IALegalRepresentative",
      "input": {
        "eventMessage": {
          "template": "minimal-ccd-event-message.json"
        }
      },
      "expectation": {
        "credentials": "IACaseworker",
        "status": 200,
        "numberOfTasksAvailable": 1
      }
    }
  }
}
